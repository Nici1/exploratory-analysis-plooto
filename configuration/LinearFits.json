{
    "bootstrap_servers": ["localhost:9092"],
    "auto_offset_reset": "latest",
    "enable_auto_commit": "True",
    "group_id": "my-group",
    "value_deserializer": "lambda x: loads(x.decode('utf-8'))",
    "file_name":"Braila_nightly.csv",
    "anomaly_detection_alg": ["LinearFit()"],
    "anomaly_detection_conf": [{
        "input_vector_size": 1,
        "warning_stages": [0.5, 0.9],
        "N": 10,
        "UL": 4,
        "LL": -4,
        "confidence_norm": 6,
        "output": ["FileOutput()"],
        "output_conf": [
            {
                "file_name": "Braila_nightly_LinearFits.csv",
                "mode": "w"
            }
        ],
        "visualization": "StatusPointsVisualization()",
        "visualization_conf":
            {
                "num_of_points": 50,
                "num_of_lines": 1,
                "linestyles":["wo"]
            }
    }]
}